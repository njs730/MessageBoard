<h1>Messages</h1>
<%= link_to "New Message", new_message_path, class: "btn btn-success" %>
<p></p>
<table class="table">
  <% @messages.each do |message| %>
    <tr >
      <td>
        <%= message.id %>
      </td>
      <td>
        <%= message.title %>
      </td>
      <!-- <td>
        <%= message.body %>
      </td> -->
      <td>
        <%= link_to "View Message", message_path(message) %>
      </td>
    </tr>
  <% end %>
</table>
